<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/appsetting_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_backg_color"
    android:clickable="true"
    tools:context="com.kt.recycleapp.java.fragment.AppSettingFragment">


    <TextView
        android:id="@+id/darkmod_tv1"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:fontFamily="@font/font3"
        android:text="다크모드"
        android:textColor="@color/main_text_color"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/darkmodonoff_bt1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12" />

    <TextView
        android:id="@+id/announce_tv1"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:fontFamily="@font/font3"
        android:text="사용설명서"
        android:textColor="@color/main_text_color"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/announce_bt1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/announce_bt1"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:background="@drawable/common_buttom_design"
        android:fontFamily="@font/font3"
        android:text="보기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/announce_tv1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/darkmodonoff_bt1"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:background="@drawable/common_buttom_design"
        android:fontFamily="@font/font3"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/darkmod_tv1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.120000005" />


</androidx.constraintlayout.widget.ConstraintLayout>